<AnchorPane prefHeight="400.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HospitalListController">
   <children>
      <ImageView fx:id="imageView2" fitHeight="400.0" fitWidth="952.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@Top-10-Hospitals-In-India-2021.jpg" />
         </image>
      </ImageView>
      <Label layoutX="284.0" layoutY="28.0" text="Search Price of seat or Hospital  Name">
         <font>
            <Font name="Anton Regular" size="11.0" />
         </font></Label>
      <TextField fx:id="searchField" layoutX="218.0" layoutY="52.0" opacity="0.75" prefHeight="25.0" prefWidth="354.0" style="-fx-background-color: #607d8b; -fx-background-radius: 45;">
         <font>
            <Font name="Tahoma" size="12.0" />
         </font></TextField>
      <TableView fx:id="tableView" layoutX="150.0" layoutY="95.0" opacity="0.82" prefHeight="280.0" prefWidth="588.0" style="-fx-background-color: #607d8b;">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor></TableView>
      <Button fx:id="clickButton" layoutX="583.0" layoutY="64.0" mnemonicParsing="false" onAction="#clickAction" opacity="0.77" prefHeight="25.0" prefWidth="155.0" style="-fx-background-color: #607d8b; -fx-background-radius: 45;" text="Click Here" />
      <Button fx:id="previousButton" layoutX="45.0" layoutY="363.0" mnemonicParsing="false" onAction="#previousAction" text="Previous page" />
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>
