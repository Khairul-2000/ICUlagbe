<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RatingController">
   <children>
      <Label layoutX="215.0" layoutY="125.0" text="Ratings (out of 5)" />
      <Button fx:id="submitButton" layoutX="380.0" layoutY="330.0" mnemonicParsing="false" onAction="#submitButtonAction" prefHeight="25.0" prefWidth="158.0" text="Submit" />
      <TextField fx:id="ratingTextField" layoutX="129.0" layoutY="157.0" prefHeight="25.0" prefWidth="324.0" />
      <Text layoutX="192.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please give your valuable feedback&#10;&#10;">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
      <Label layoutX="230.0" layoutY="206.0" text="How was your experience?" />
      <RadioButton fx:id="badButton" layoutX="181.0" layoutY="269.0" mnemonicParsing="false" text="BAD" />
      <RadioButton fx:id="goodButton" layoutX="307.0" layoutY="269.0" mnemonicParsing="false" text="GOOD" />
      <RadioButton fx:id="excellentButton" layoutX="433.0" layoutY="269.0" mnemonicParsing="false" text="Excellent" />
      <Button fx:id="closeButton" layoutX="80.0" layoutY="330.0" mnemonicParsing="false" onAction="#closeAction" prefHeight="25.0" prefWidth="141.0" text="Close" />
   </children>
</AnchorPane>
